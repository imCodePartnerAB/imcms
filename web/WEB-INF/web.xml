    <display-name>imCMS</display-name>

    <filter>
        <description>Checking for answer from IDP server</description>
        <filter-name>SSOFilter</filter-name>
        <filter-class>imcode.server.user.saml2.SAMLSPFilter</filter-class>
        <init-param>
            <param-name>excludedUrlPattern</param-name>
            <param-value>.*\.jpg</param-value>
        </init-param>
        <init-param>
            <param-name>idProviderSSOUrl</param-name>
            <param-value>https://m00-mg-local.testidp.funktionstjanster.se/samlv2/idp/req/0/0</param-value>
        </init-param>
        <init-param>
            <param-name>spProviderId</param-name>
            <param-value>${server.name}</param-value>
        </init-param>
        <init-param>
            <param-name>acsUrl</param-name>
            <param-value>${server.name}/acs</param-value>
        </init-param>
        <init-param>
            <param-name>logoutUrl</param-name>
            <param-value>/logout</param-value>
        </init-param>
    </filter>

    <filter>
        <description>Checks for multiple user login</description>
        <filter-name>ImcmsMultipleUserLoginFilter</filter-name>
        <filter-class>com.imcode.imcms.servlet.ImcmsMultipleUserLoginFilter</filter-class>
    </filter>

    <filter>
        <description>Setting up objects in request for api usage.</description>
        <filter-name>ImcmsSetupFilter</filter-name>
