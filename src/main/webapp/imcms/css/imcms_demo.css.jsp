<%@ page
    
    contentType="text/css"
    pageEncoding="UTF-8"
    
%>

H1 { font: bold medium Verdana,Geneva,sans-serif; color:#009; }
TD, LI { font: x-small Verdana,Geneva,sans-serif; color:#000; }
.text { font: x-small Verdana,Geneva,sans-serif; color:#000; margin-bottom:1em; }
.small { font: xx-small Verdana,Geneva,sans-serif; color:#000; }
PRE, TT { font: x-small "Courier New", Courier, monospace; color:#888; }

A:link    { color:#009; text-decoration: none; }
A:visited { color:#009; text-decoration: none; }
A:active  { color:#c00; text-decoration: underline; }
A:hover   { color:#00f; text-decoration: underline; }

LI { padding-bottom:5px; }

