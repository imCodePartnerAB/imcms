<?xml version="1.0" encoding="UTF-8"?>
<taglib xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://java.sun.com/xml/ns/j2ee"
		xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
		version="2.0">

	<tlib-version>1.0</tlib-version>
	<short-name>archive</short-name>
	<uri>archive</uri>

	<tag>
		<name>libraryChildren</name>
		<tag-class>com.imcode.imcms.imagearchive.tag.LibraryChildren</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>library</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>currentLibrary</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>libraries</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<tag>
		<name>param</name>
		<tag-class>com.imcode.imcms.imagearchive.tag.param.ParamTag</tag-class>
		<body-content>empty</body-content>

		<attribute>
			<name>value</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>params</name>
		<tag-class>com.imcode.imcms.imagearchive.tag.param.ParamsTag</tag-class>
		<body-content>JSP</body-content>

		<attribute>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<tag>
		<name>pagination</name>
		<tag-class>com.imcode.imcms.imagearchive.tag.pagination.PaginationTag</tag-class>
		<body-content>JSP</body-content>

		<attribute>
			<name>pageUrl</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>onchange</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>pag</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>currentPage</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>pageCount</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>pagesBeforeEllipse</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>capacity</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>nextText</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>prevText</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>contClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>contStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>currentPageClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>currentPageStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<function>
		<name>newlineToBr</name>
		<function-class>com.imcode.imcms.imagearchive.tag.func.Functions</function-class>
		<function-signature>java.lang.String newlineToBr(java.lang.String)</function-signature>
	</function>
	<function>
		<name>abbreviate</name>
		<function-class>com.imcode.imcms.imagearchive.tag.func.Functions</function-class>
		<function-signature>java.lang.String abbreviate(java.lang.String, int)</function-signature>
	</function>
	<function>
		<name>join</name>
		<function-class>com.imcode.imcms.imagearchive.tag.func.Functions</function-class>
		<function-signature>java.lang.String join(java.util.Collection, java.lang.String)</function-signature>
	</function>
	<function>
		<name>isInArchive</name>
		<function-class>com.imcode.imcms.imagearchive.tag.func.Functions</function-class>
		<function-signature>boolean isInArchive(com.imcode.imcms.imagearchive.dto.LibraryEntryDto,
			javax.servlet.jsp.PageContext)
		</function-signature>
	</function>
	<function>
		<name>doubleToFractionsString</name>
		<function-class>com.imcode.imcms.imagearchive.tag.func.Functions</function-class>
		<function-signature>java.lang.String doubleToFractionsString(java.lang.Double)</function-signature>
	</function>

</taglib>
