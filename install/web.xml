<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
    <servlet>
        <servlet-name>
            init
        </servlet-name>
        <servlet-class>
            MainInitServlet
        </servlet-class>
	<load-on-startup>
            1
	</load-on-startup>		
    </servlet>
	
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>

    <welcome-file-list>                                                   
	<welcome-file>index.jsp</welcome-file> 
	<welcome-file>index.htm</welcome-file> 
	<welcome-file>index.html</welcome-file>                               
    </welcome-file-list> 
</web-app>



